  "version": "1.0.0",
  "description": "minimal-config",
  "type": "module",
  "scripts": {
    "dev": "npx @lwc-garden/core dev"
  },
  "pnpm": {
    "overrides": {
      "@lwrjs/module-registry": "npm:@lukethacoder/lwrjs-module-registry"
    }
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@lukethacoder/lwrjs-module-registry": "^0.12.2",
    "@lwc-garden/core": "1.0.1",
    "@lwc/engine-dom": "^6.5.0",
    "@lwc/synthetic-shadow": "^6.5.0",
    "@lwrjs/client-modules": "^0.12.2",
    "@lwrjs/config": "^0.12.2",
    "@lwrjs/core": "^0.12.2",
    "@lwrjs/label-module-provider": "^0.12.2",
    "@lwrjs/loader": "^0.12.2",
    "@lwrjs/markdown-view-provider": "^0.12.2",
    "@lwrjs/o11y": "^0.12.2",
    "@lwrjs/router": "^0.12.2",
    "@lwrjs/types": "^0.12.2",
    "@salesforce-ux/design-system": "^2.23.2",
    "lightning-base-components": "1.22.1-alpha",
    "lwc": "^6.5.0",
    "lwr": "^0.12.2"
  },
  "lint-staged": {
    "**/*.{css,html,js,json,md,yaml,yml}": [
      "prettier --write"
    ],
    "**/lwc/**/*.js": [
